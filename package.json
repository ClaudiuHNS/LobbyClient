{
  "name": "leaguesandbox-lobbyclient",
  "version": "0.0.1",
  "description": "Lobby client for the League Sandbox project.",
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && copyfiles main.js ./dist && copyfiles package.json ./dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "start": "electron . --dev",
    "package:win": "electron-packager dist leaguesandbox-lobbyclient --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Lobby Client\""
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "2.3.5",
    "apollo-link": "1.2.2",
    "apollo-link-context": "1.0.8",
    "apollo-link-http": "1.5.4",
    "asar": "^0.14.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "materialize-css": "1.0.0-rc.2",
    "vue": "^2.5.16",
    "vue-apollo": "3.0.0-beta.19",
    "vue-class-component": "^6.0.0",
    "vue-material": "1.0.0-beta-10.2",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "3.0.1",
    "electron-packager": "^12.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-rc.5",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "electron": "2.0.5",
    "electron-packager": "^12.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16"
  }
}
