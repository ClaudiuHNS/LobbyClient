{
  "name": "leaguesandbox-lobbyclient",
  "version": "0.0.1",
  "description": "Lobby client for the League Sandbox project.",
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && copyfiles main.js ./dist && copyfiles package.json ./dist && yarn --production --cwd ./dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "start-server-and-test serve http://localhost:8080 cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start": "electron . --dev",
    "package:win": "electron-packager dist leaguesandbox-lobbyclient --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Lobby Client\"",
    "package:mac": "electron-packager dist --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds"
  },
  "dependencies": {
    "jszip": "^3.1.5",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@types/socket.io-client": "^1.4.32",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-rc.10",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.10",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/test-utils": "^1.0.0-beta.20",
    "asar": "^0.14.3",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "electron": "2.0.6",
    "electron-packager": "^12.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "socket.io-client": "^2.1.1",
    "start-server-and-test": "^1.7.0",
    "typescript": "^3.0.1",
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-material": "1.0.0-beta-10.2",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "3.0.1"
  }
}
